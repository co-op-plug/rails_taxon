<p id="notice"><%= notice %></p>

<h1>Node Parents</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @node_parents.each do |node_parent| %>
      <tr>
        <td><%= link_to 'Show', node_parent %></td>
        <td><%= link_to 'Edit', edit_node_parent_path(node_parent) %></td>
        <td><%= link_to 'Destroy', node_parent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Node Parent', new_node_parent_path %>
