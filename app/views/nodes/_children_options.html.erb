<%= f.collection_select "parent_ancestors(#{node.depth}i)",
                        node.self_and_siblings.where(default_params),
                        :id,
                        :name,
                        { selected: node.id, include_blank: 'Select', label: false, on: { wrapper_all: false, wrapper_input_id: true } },
                        { data: { type: node.class.base_class.name, index: f.index, title: 'parent_ancestors' } } %>
